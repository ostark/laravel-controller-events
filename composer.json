{
    "name": "ostark/laravel-package-events",
    "description": "Provides Events before and after a Controller Action is called",
    "keywords": [
        "laravel",
        "events"
    ],
    "homepage": "https://github.com/ostark/laravel-package-events",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Stark",
            "email": "os@fortrabbit.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.0",
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.18",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ostark\\LaravelControllerEvents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ostark\\LaravelControllerEvents\\Tests\\": "tests"
        }
    },
    "scripts": {},
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ostark\\LaravelControllerEvents\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
